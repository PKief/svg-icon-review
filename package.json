{
  "name": "vscode-icon-theme-reviewer",
  "displayName": "VS Code Icon Theme Reviewer",
  "version": "0.1.0",
  "description": "Able to review pull requests of icon themes in VS Code",
  "main": "./lib/index.js",
  "module": "./lib/index.js",
  "bin": {
    "vscode-icon-theme-reviewer": "./bin/cli.js"
  },
  "scripts": {
    "start": "node ./bin/cli.js angular.svg",
    "build": "tsc -p tsconfig.json",
    "test": "jest",
    "lint": "eslint -c .eslintrc.json --ext .ts ./src/**/*.ts",
    "changelog": "npx changelog-machine --config changelog.config.json",
    "prepublish": "npm run build",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "files": [
    "lib/"
  ],
  "keywords": [
    "VS Code",
    "VS Code extension",
    "Extensions",
    "SVG icon",
    "Preview"
  ],
  "engines": {
    "node": ">=4.2.0"
  },
  "author": {
    "name": "Philipp Kief",
    "email": "philipp.kief@gmx.de",
    "url": "https://pkief.github.io"
  },
  "funding": "https://github.com/sponsors/pkief",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/PKief/vscode-icon-theme-reviewer/issues"
  },
  "homepage": "https://github.com/PKief/vscode-icon-theme-reviewer/blob/main/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PKief/vscode-icon-theme-reviewer.git"
  },
  "devDependencies": {
    "@types/chroma-js": "^2.1.3",
    "@types/glob": "^7.2.0",
    "@types/is-glob": "^4.0.2",
    "@types/jest": "^27.4.0",
    "@types/js-yaml": "^4.0.5",
    "@types/minimist": "^1.2.2",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "changelog-machine": "^1.0.2",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.0",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "glob": "^7.2.0",
    "is-glob": "^4.0.3",
    "is-svg": "^4.3.2",
    "js-yaml": "^4.1.0",
    "minimist": "^1.2.5",
    "puppeteer": "^21.5.0"
  }
}
